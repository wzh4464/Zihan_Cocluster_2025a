}

@article{lee2006WntFrizzledSignaling,
  title = {Wnt/Frizzled Signaling Controls {{C}}. Elegans Gastrulation by Activating Actomyosin Contractility},
  author = {Lee, Jen-Yi and Marston, Daniel J. and Walston, Timothy and Hardin, Jeff and Halberstadt, Ari and Goldstein, Bob},
  year = {2006},
  month = oct,
  journal = {Current Biology},
  volume = {16},
  number = {20},
  pages = {1986--1997},
  issn = {09609822},
  doi = {10.1016/j.cub.2006.08.090},
  urldate = {2024-06-20},
  abstract = {Background: Embryonic patterning mechanisms regulate the cytoskeletal machinery that drives morphogenesis, but there are few cases where links between patterning mechanisms and morphogenesis are well understood. We have used a combination of genetics, in vivo imaging, and cell manipulations to identify such links in C. elegans gastrulation. Gastrulation in C. elegans begins with the internalization of endodermal precursor cells in a process that depends on apical constriction of ingressing cells. Results: We show that ingression of the endodermal precursor cells is regulated by pathways, including a Wnt-Frizzled signaling pathway, that specify endodermal cell fate. We find that Wnt signaling has a role in gastrulation in addition to its earlier roles in regulating endodermal cell fate and cell-cycle timing. In the absence of Wnt signaling, endodermal precursor cells polarize and enrich myosin II apically but fail to contract their apical surfaces. We show that a regulatory myosin light chain normally becomes phosphorylated on the apical side of ingressing cells at a conserved site that can lead to myosin-filament formation and contraction of actomyosin networks and that this phosphorylation depends on Wnt signaling. Conclusions: We conclude that Wnt signaling regulates C. elegans gastrulation through regulatory myosin lightchain phosphorylation, which results in the contraction of the apical surface of ingressing cells. These findings forge new links between cell-fate specification and morphogenesis, and they represent a novel mechanism by which Wnt signaling can regulate morphogenesis. 背景：胚胎图式化机制调控驱动形态发生的细胞骨架机器，但在少数情况下，图式化机制和形态发生之间的联系是完全理解的。我们结合遗传学、体内成像和细胞操作，鉴定了在秀丽隐杆线虫（C. elegans）原肠胚形成中的这种联系。秀丽隐杆线虫的原肠胚形成始于内胚层前体细胞的内化，这一过程依赖于内陷细胞的顶端收缩。 结果：我们表明，内胚层前体细胞的内陷受包括Wnt-Frizzled信号通路在内的多条途径调控，这些途径决定内胚层细胞的命运。我们发现，除了在调控内胚层细胞命运和细胞周期时间上的早期作用外，Wnt信号在原肠胚形成中也发挥作用。在缺乏Wnt信号的情况下，内胚层前体细胞极化并在顶端富集肌球蛋白II，但未能收缩其顶端表面。我们展示了一种调节性肌球蛋白轻链通常在内陷细胞的顶端侧面被磷酸化，磷酸化发生在一个保守的位点，该位点可以导致肌球蛋白丝的形成和肌动蛋白-肌球蛋白网络的收缩，这一磷酸化依赖于Wnt信号。 结论：我们得出结论，Wnt信号通过调节性肌球蛋白轻链磷酸化调控秀丽隐杆线虫的原肠胚形成，从而导致内陷细胞顶端表面的收缩。这些发现建立了细胞命运决定和形态发生之间的新联系，并代表了一种Wnt信号调控形态发生的新机制。},
  langid = {english},
  keywords = {/unread},
  annotation = {影响因子: 9.2},
  file = {/Users/zihan/Zotero/storage/4CH9VHXV/Lee 等 - 2006 - Wntfrizzled signaling controls C. elegans gastrulation by activating actomyosin contractility.pdf}
}

